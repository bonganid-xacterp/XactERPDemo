          </TableColumn>
          <TableColumn name="st01_mast.status" colName="status" sqlType="VARCHAR(20)" required="1" notNull="1" fieldId="14" sqlTabName="st01_mast" text="Status" tabIndex="10">
            <Edit width="6" scroll="1"/>
            <LStr text="Status"/>
          </TableColumn>
        </Table>
      </Page>
    </Folder>
