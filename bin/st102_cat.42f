      <Label name="st02_mast_cat_code" width="8" text="Category Code" posY="2" posX="1" gridWidth="8">
        <LStr text="Category Code"/>
      </Label>
      <FormField name="st02_cat.cat_code" colName="cat_code" sqlType="VARCHAR(30)" fieldId="0" sqlTabName="st02_cat" tabIndex="1">
        <ButtonEdit width="18" action="find" image="zoom" scroll="1" posY="2" posX="10" gridWidth="20"/>
      </FormField>
      <Label name="st02_mast_cat_name" width="8" text="Category Name" posY="2" posX="36" gridWidth="8">
        <LStr text="Category Name"/>
      </Label>
      <FormField name="st02_cat.cat_name" colName="cat_name" sqlType="VARCHAR(120)" required="1" notNull="1" fieldId="1" sqlTabName="st02_cat" tabIndex="2">
        <Edit width="23" scroll="1" posY="2" posX="45" gridWidth="23"/>
      </FormField>
      <Label name="st02_mast_description" width="8" text="Description" posY="3" posX="1" gridWidth="8">
        <LStr text="Description"/>
