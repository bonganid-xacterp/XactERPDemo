      </PhantomFormField>
      <PhantomFormField name="st02_cat.updated_at" colName="updated_at" sqlType="DATETIME YEAR TO FRACTION(4)" fieldId="8" sqlTabName="st02_cat">
        <PhantomEdit width="1"/>
      </PhantomFormField>
    </Grid>
    <Folder name="fld_st_cat_grp" hidden="1">
      <Page name="st_cat_trans" text="Stock" image="fa-sitemap">
        <LStr text="Stock"/>
        <Table pageSize="5" name="demoappdb1" tabName="arr_stock">
