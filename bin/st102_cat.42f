<?xml version='1.0' encoding='windows-1252'?>
<Form name="st102_cat.tmp" build="3.20.14" sqlDbName="demoappdb" text="Stock Category" windowStyle="child" width="75" height="10">
  <LStr text="Stock Category"/>
  <VBox name="vbox1">
