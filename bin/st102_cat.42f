      <Label name="st02_mast_status" width="8" text="Status" posY="3" posX="36" gridWidth="8">
        <LStr text="Status"/>
      </Label>
      <FormField name="st02_cat.status" colName="status" sqlType="VARCHAR(20)" required="1" notNull="1" fieldId="3" sqlTabName="st02_cat" tabIndex="4" defaultValue="1">
        <ComboBox width="8" scroll="1" posY="3" posX="45" gridWidth="10">
          <Item name="1" text="Inactive">
            <LStr text="Inactive"/>
          </Item>
          <Item name="0" text="Active">
            <LStr text="Active"/>
          </Item>
          <Item name="-1" text="Archived">
            <LStr text="Archived"/>
          </Item>
        </ComboBox>
      </FormField>
      <PhantomFormField name="st02_cat.id" colName="id" noEntry="1" sqlType="BIGINT" required="1" notNull="1" fieldId="4" sqlTabName="st02_cat">
        <PhantomEdit width="1"/>
      </PhantomFormField>
      <PhantomFormField name="st02_cat.created_at" colName="created_at" sqlType="DATETIME YEAR TO FRACTION(4)" required="1" notNull="1" fieldId="5" sqlTabName="st02_cat">
